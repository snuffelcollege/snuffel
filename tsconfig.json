{
	"compilerOptions": {
		"noImplicitAny": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"esnext",
			"dom",
			"scripthost"
		],
		"sourceMap": true,
		"resolveJsonModule": true,
		"module": "es6",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"allowJs": false
	},
	"exclude": [
		"node_modules"
	]
}
